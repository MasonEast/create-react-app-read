        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "node",
        "jsx": "react"
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "build",
        "mock"
    ]
}